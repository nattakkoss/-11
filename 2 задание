#include <iostream>
using  namespace std;  
int main() 
{
    int a[10] = {1,2,3,4,5,6,7,8,9} ; 
    int b[10][10];
    for (int i=1; i<10; i++)
   { 
       for (int j=1;j<10;j++)
      {  
           b[i][j] = i*j; 
           cout.width(3);
           cout << b[i][j] << " "; 
       }
         cout << endl; 
       
   } 
   return 0;
}
